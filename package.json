{
  "name": "notabug",
  "version": "0.61.2",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "go1dfish",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/notabugio/notabug.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "mirror": "node server.js --lmdb --port 3333 --peer https://notabug.io/gun --render",
    "uidev": "node server.js --dev --lmdb --port 3333 --peer https://notabug.io/gun --render",
    "start": "pm2 start cluster.lmdb.pm2.json",
    "restart": "pm2 restart http && pm2 restart index",
    "reload": "pm2 reload http && pm2 reload index",
    "stop": "pm2 stop http && pm2 stop index && pm2 stop relay",
    "sync": "node server.js --lmdb --sync --peer https://notabug.io/gun",
    "backindex": "node --max-old-space-size=8192 server.js --evict --lmdb --backindex",
    "logs": "pm2 logs",
    "monit": "pm2 monit",
    "relay": "pm2 start relay.pm2.json",
    "updatepm2": "pm2 update",
    "dev": "node server.js --lmdb --port 3333 --peer https://notabug.io/gun --render --index --tabulate",
    "build": "yarn buildui && yarn buildserver",
    "buildui": "cross-env-shell BABEL_ENV=client \"rimraf build && parcel build -d build src/index.html\" && yarn tohtdocs",
    "buildserver": "cross-env-shell BABEL_ENV=server \"rimraf server-build && parcel build -d server-build -o index.js -t node --no-minify src/server-peer.js\"",
    "clean": "rimraf build && rimraf server-build",
    "tohtdocs": "copyfiles --up 1 build/* build/**/* htdocs"
  },
  "alias": {
    "argon2": "./src/vendor/argon2.stub.js"
  },
  "dependencies": {
    "@notabug/gun-http": "^0.1.2",
    "@notabug/gun-lmdb": "^0.2.2",
    "@notabug/gun-localforage": "^0.1.1",
    "@notabug/gun-openstack-swift": "^0.2.0",
    "@notabug/gun-receiver": "^0.5.1",
    "@notabug/gun-redis": "^0.10.0",
    "@notabug/peer": "^0.23.4",
    "@tippy.js/react": "^2.1.2",
    "argon2-browser": "^1.5.3",
    "command-line-args": "^5.1.1",
    "compression": "^1.7.4",
    "detect-node": "^2.0.4",
    "express": "^4.16.4",
    "express-cache-headers": "^0.1.4",
    "express-history-api-fallback": "^2.2.1",
    "gun": "^0.2019.428",
    "jdenticon": "^2.1.1",
    "lodash.debounce": "^4.0.8",
    "parcel-plugin-static-files-copy": "^2.0.0",
    "pm2": "^3.5.0",
    "query-string": "^6.5.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-chatview": "^0.2.5",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-instagram-embed": "^1.5.0",
    "react-player": "^1.11.0",
    "react-popper": "^1.3.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-spinkit": "^3.0.0",
    "react-timeago": "^4.4.0",
    "react-visibility-sensor": "^5.0.2",
    "serialize-javascript": "^1.7.0",
    "snudown-js": "^3.1.5",
    "text-encoding": "^0.7.0",
    "uri-js": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "less": "^3.9.0",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^2.6.3"
  },
  "browserslist": [
    ">0.05%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
